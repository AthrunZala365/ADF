					},
					"SQLServerOnPremLinkedService01_connectionString": {
						"value": "[parameters('SQLServerOnPremLinkedService01_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1LinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1LinkedService_properties_typeProperties_url')]"
