					},
					"LinkedServerSHR02_properties_typeProperties_database": {
						"value": "[parameters('LinkedServerSHR02_properties_typeProperties_database')]"
					},
					"LinkedServerSHR02_properties_typeProperties_userName": {
						"value": "[parameters('LinkedServerSHR02_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
