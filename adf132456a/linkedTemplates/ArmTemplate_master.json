		"AzureDataLakeStorage1LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://destiny1234.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault123ab.vault.azure.net/"
		},
		"LinkedServerSHR02_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-N1A7I1M\\DEV001"
		},
		"LinkedServerSHR02_properties_typeProperties_database": {
			"type": "string",
